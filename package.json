  },
  "homepage": "https://github.com/protoman92/chatbot-engine#readme",
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/gulp": "^4.0.6",
    "@types/mocha": "^5.2.6",
    "@types/redis": "^2.8.12",
    "bignumber.js": "^8.1.1",
    "expect.js": "^0.3.1",
    "gulp": "^4.0.2",
    "gulp-run": "^1.7.1",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0",
